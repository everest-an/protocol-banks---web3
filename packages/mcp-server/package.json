{
  "name": "@protocolbanks/mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for ProtocolBanks - AI Agent subscription payments via x402 protocol",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "protocolbanks-mcp": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "config"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai-agent",
    "payments",
    "x402",
    "subscriptions",
    "protocolbanks"
  ],
  "author": "ProtocolBanks",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@protocolbanks/sdk": "workspace:*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
